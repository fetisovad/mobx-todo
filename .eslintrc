{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "ecmaFeatures": {
      "impliedStrict": true
    },
    "requireConfigFile": false,
    "sourceType": "module",
    "babelOptions": {
      "presets": [
        "@babel/preset-env",
        ["@babel/preset-react", {
          "development": true,
          "runtime": "automatic"
        }]
      ],
      "plugins": [
        ["@babel/plugin-proposal-decorators", {"legacy": true}]
      ]
    }
  }
}